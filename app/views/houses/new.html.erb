<h1>物件登録</h1>

<%= form_with(model:@houses, local:true) do |house| %>
  <div class= "house_building">
    <%= house.label :物件名 %>
    <%= house.text_field :building %>
  </div>
  <div class= "house_address">
    <%= house.label :住所 %>
    <%= house.text_field :address %>
  </div>
  <div class= "house_years">
    <%= house.label :築年数 %>
    <%= house.text_field :years %>
  </div>
  <div class= "house_other">
    <%= house.label :備考 %>
    <%= house.text_field :other %>
  </div>
<% end %>
