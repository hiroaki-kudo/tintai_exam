<h1>物件一覧</h1>

<table>
  <tr>
     <th>物件名</th>
     <th>賃料</th>
     <th>住所</th>
     <th>築年数</th>
     <th>備考</th>
  </tr>
    <% @house.each do |house| %>
      <tr>
        <td><%= house.building %></td>
        <td><%= house.money %>円</td>
        <td><%= house.address %></td>
        <td><%= house.years %>年</td>
        <td><%= house.other %></td>
        <td><%= link_to '詳細', house_path(house.id) %></td>
        <td><%= link_to '編集', edit_house_path(house.id) %></td>
        <td><%= link_to '削除', house_path(house.id), method: :delete %></td>
      </tr>
    <% end %>
</table>

<p><%= link_to '物件を登録する', new_house_path %></p>
